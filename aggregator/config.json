{
    "mqttServer": "http://localhost",
    "mqttPort": 1883,
    "publications": [
        {
            "channel": "sync agg 1",
            "message": "{\"sync\":\"0\"}",
            "interval": 3600000

		}
	],
    "subscriptions": ["testsensor", "sensor1", "sensor999", "sensor2", "sensor123", "sensorABC", "sensorXYZ"],
    "aggregator_id": "aggregator1",
    "reading_interval": 60000,
    "coordinator_groups": [
        {
            "group": "agg_1",
            "interval": 300000,
            "sensors": ["sensor1", "sensor2", "sensor123"],
            "operations": {
                "min": true,
                "max": true,
                "count": true,
                "mean": true
            }

		}, {
            "group": "min_max2",
            "interval": 300000,
            "sensors": ["sensor1"],
            "operations": {
                "min": true,
                "max": true,
                "count": true,
                "mean": false
            }
		}
	]
}
